=-> './index.css'
=-> 'react' React
=-> 'react-dom' ReactDOM
=-> './styles' S

= (: (createElement e)) React

= App -> ((: name))
    e S.Container (: ())
        e S.Hello (: (color 'green')) 'Hello'
        e S.Name (: ()) name

ReactDOM.render 
    e App (: (name 'John')) 
    document.getElementById 'root'