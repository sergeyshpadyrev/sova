=-> './index.css'
=-> 'react' React
=-> 'react-dom' ReactDOM
=-> './styles' S

= (: (createElement e)) React

= App -> ((: name))
    = (:: count setCount) (React.useState 0)
    e S.Container (: ())
        e S.Hello (: (color 'green')) 'Hello, '
        e S.Name (: ()) name
        e S.IncrementCount 
            : (onClick (-> () (setCount (+ count 1))))
            'Press me'
        e S.Count (: ()) count

ReactDOM.render 
    e App (: (name 'John')) 
    document.getElementById 'root'