=-> 'react' React
=-> 'react-native' (: View Text)
=-> './styles' S

= (: (createElement e)) React

= App -> ()
    = (:: count setCount) (React.useState 0)
    e S.Container (: ())
        e S.Hello (: (color 'green')) 'Hello'
        e S.IncrementCount 
            : (onPress (-> () (setCount (+ count 1))))
            'Press me'
        e S.Count (: ()) count

<-= App